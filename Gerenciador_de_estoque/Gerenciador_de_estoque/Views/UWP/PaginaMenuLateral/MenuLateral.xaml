<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Gerenciador_de_estoque.Views.UWP.PaginaMenuLateral.MenuLateral"
             xmlns:local="clr-namespace:Gerenciador_de_estoque.Views.UWP"
                  Title="teste">
    
    <!--Menu-->
    <MasterDetailPage.Master>
        <ContentPage>
            <StackLayout>
                <Label Text="teste"/>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
    
    <!--Conteudo-->
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:ViewItem/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>